default_schematic: "logger"

version: "0"

network {
    schematics: [{
        name: "logger",
        instances {
          logger {
            id: "wafl::log"
          }
        },
        connections: [
            {
              from { instance: "wafl::schematic_input", port: "input" },
              to { instance: "logger", port: "input" },
            },
            {
              from { instance: "logger", port: "output" },
              to { instance: "wafl::schematic_input", port: "output" },
            },
          ],
      }],
  }



