format: 0
network:
  schematics:
    - name: refself-parent
      instances:
        child_ref:
          id: self::child
      connections:
        - <>[parent_input] => child_ref[child_input]
        - child_ref[child_output] => <>[parent_output]
    - name: child
      instances:
        log:
          id: wafl::core::log
      connections:
        - <>[child_input] => log[input]
        - log[output] => <>[child_output]
