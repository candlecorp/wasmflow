---
name: mytypes
metadata:
  version: 0.1.0
package:
  registry:
    host: $DOCKER_REGISTRY
    namespace: default
kind: wick/types@v1
types:
  - name: HttpVersion
    kind: wick/type/enum@v1
    variants:
      - name: HTTP_1_0
        value: '1.0'
      - name: HTTP_1_1
        value: '1.1'
      - name: HTTP_2_0
        value: '2.0'
  - name: StatusCode
    kind: wick/type/enum@v1
    variants:
      - name: OK
        value: '200'
      - name: Created
        value: '201'
  - name: HttpResponse
    kind: wick/type/struct@v1
    fields:
      - name: version
        type: HttpVersion
        description: HTTP version enum
      - name: status
        type: StatusCode
        description: status code enum
      - name: headers
        type: '{string: string[]}'
        description: All response headers. Supports duplicates.
      - name: body
        type: bytes
        description: Response body in bytes. optional.
