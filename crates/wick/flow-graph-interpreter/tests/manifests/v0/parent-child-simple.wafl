version: 0
network:
  schematics:
    - name: nested_parent
      instances:
        child_ref:
          id: self::child
      connections:
        - <>[parent_input] => child_ref[child_input]
        - child_ref[child_output] => <>[parent_output]
    - name: child
      connections:
        - <>[child_input] => <>[child_output]
