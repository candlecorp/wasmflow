
{{~#switch kind ~}}
  {{~#case "Named"~}}
    {{~#switch name.value~}}
      {{~#case "string"}}`{{~name.value~}}`{{/case~}}
      {{~#case "i8"}}`{{~name.value~}}`{{/case~}}
      {{~#case "u8"}}`{{~name.value~}}`{{/case~}}
      {{~#case "i16"}}`{{~name.value~}}`{{/case~}}
      {{~#case "u16"}}`{{~name.value~}}`{{/case~}}
      {{~#case "i32"}}`{{~name.value~}}`{{/case~}}
      {{~#case "u32"}}`{{~name.value~}}`{{/case~}}
      {{~#case "i64"}}`{{~name.value~}}`{{/case~}}
      {{~#case "u64"}}`{{~name.value~}}`{{/case~}}
      {{~#case "f32"}}`{{~name.value~}}`{{/case~}}
      {{~#case "f64"}}`{{~name.value~}}`{{/case~}}
      {{~#case "bool"}}`{{~name.value~}}`{{/case~}}
      {{~#case "any"}}`{{~name.value~}}`{{/case~}}
      {{~#default~}}[`{{~name.value~}}`](#{{lowerCase name.value}}){{~/default~}}
    {{~/switch~}}
  {{~/case~}}
  {{~#case "MapType"~}}
    `{` {{> expand-type keyType}} `: ` {{> expand-type valueType}} `}`
  {{~/case~}}
  {{~#case "ListType"~}}
    {{> expand-type type}}[]
  {{~/case~}}
  {{~#case "Optional"~}}
    {{~>expand-type type}}
  {{~/case~}}
  {{~#default~}}
  unknown type
  {{~/default~}}
{{~/switch~}}