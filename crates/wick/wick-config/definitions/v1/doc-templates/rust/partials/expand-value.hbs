
{{#switch kind ~}}
  {{~#case "StringValue"~}}
  "".to_owned()
  {{~/case~}}
  {{~#case "ObjectValue"~}}
    {{~#if fields ~}}
      {{~panic "Unhandled default" . ~}}
    {{~/if~}}
    {}
  {{~/case~}}
  {{~#case "ListValue"~}}
    {{~#if value ~}}
      {{~panic "Unhandled default" . ~}}
    {{~/if~}}
    []
  {{~/case~}}
  {{~#default~}}
  {{~panic "unhandled value" . ~}}
  {{~/default~}}
{{~/switch}}