"v1.{{name.value}}": {
  "oneOf" : [
    {{#withAnnotation "shortform" ~}}
      { "type":"string" },
    {{~/withAnnotation}}
    {{#withAnnotation "shortformonly"}}
      { "type":"string" }
    {{else}}
      {{#join types "," ~}}
      {"$ref": "#/$defs/v1.{{name.value}}"}
      {{~/join}}
    {{/withAnnotation}}
  ]
},
