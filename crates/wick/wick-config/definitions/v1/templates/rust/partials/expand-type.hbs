
{{#switch kind ~}}
  {{#case "Named"}}
    {{#switch name.value}}
      {{#case "LocationReference"}}crate::v1::helpers::LocationReference{{/case}}
      {{#case "LiquidJsonValue"}}liquid_json::LiquidJsonValue{{/case}}
      {{#case "Glob"}}crate::v1::helpers::Glob{{/case}}
      {{#case "any"}}Value{{/case}}
      {{#case "String"}}StringType{{/case}}
      {{#case "string"}}String{{/case}}
      {{#default}}{{name.value}}{{/default}}
    {{/switch}}
  {{/case}}
  {{#case "MapType"}}
    HashMap<{{> expand-type keyType}}, {{> expand-type valueType}}>
  {{/case}}
  {{#case "ListType"}}
    Vec<{{> expand-type type}}>
  {{/case}}
  {{#case "Optional"}}
    Option<{{>expand-type type}}>
  {{/case}}
  {{#default}}
  unknown
  {{/default}}
{{~/switch}}

