# yaml-language-server: $schema=../../../json-schema/manifest.json
---
format: 1
metadata:
  version: '1'
import:
  foo:
    kind: Mesh
    id: bar
  bar:
    kind: Wasm
    reference: bar
operations:
  - name: logger
    instances:
      logger: wafl::log
      SENDER:
        id: core::sender
        config:
          output: '123456789'
          type: string
    flow:
      - <>.input -> <>.output
