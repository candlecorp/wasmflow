---
version: 0
network:
  collections:
    - namespace: wapc
      kind: WaPC
      reference: ./crates/integration/test-baseline-component/build/baseline.signed.wasm
  schematics:
    - name: wapc_component
      collections:
        - wapc
      instances:
        validate:
          id: wapc::validate
        log:
          id: wafl::core::log
      connections:
        - <> => log[input]
        - log[output] => validate[input]
        - validate[output] => <>
