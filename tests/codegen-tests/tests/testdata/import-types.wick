---
kind: wick/component@v1
name: 'test'
import:
  - name: http
    component:
      kind: wick/component/types@v1
      ref: ./types-config.wick
  - name: AAA
    component:
      kind: wick/component/types@v1
      ref: ./types-config.wick
  - name: ZZZ
    component:
      kind: wick/component/types@v1
      ref: ./types-config.wick
types:
  - name: LocalStruct
    type: struct
    fields:
      - name: field1
        type: string
      - name: inner
        type: LocalStructInner
  - name: LocalStructInner
    type: struct
    fields:
      - name: field1
        type: string
      - name: field2
        type: string
component:
  kind: wick/component/composite@v1
  operations:
    - name: testop
      with:
        - name: A
          type: string
        - name: B
          type: u32
      inputs:
        - name: message
          type: http::HttpResponse
      outputs:
        - name: output
          type: string
      flow:
        - <> -> test::whatever[A].input
        - A.output -> <>
