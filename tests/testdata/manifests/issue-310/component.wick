kind: wick/component@v1
name: my_component
metadata:
  description: A component that connects to a Postgres database
  version: 0.0.1
  authors:
    - 'Wick Maintainers'
  vendors:
    - 'Candle Corporation'
  licenses:
    - Apache-2.0
import:
  - name: baseline
    component:
      kind: wick/component/manifest@v1
      ref: 'registry.candle.dev/test-component/baseline:1'
      with:
        default_err: 'error from issue-310'
component:
  kind: wick/component/composite@v1
  operations:
    - name: add
      flow:
        - <>.left -> baseline::add[a].left
        - <>.right -> a.right
        - a.output -> <>.output
