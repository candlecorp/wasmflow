pub mod types {
{{#ifEmpty types}}
  // no additional types
{{else}}
  {{#each types}}
    {{#switch type }}
      {{#case "struct"}}{{> struct-definition . }}{{/case}}
      {{#case "enum"}}{{> enum-definition . }}{{/case}}
      {{#default}}{{ panic "Unhandled type" }}{{/default}}
    {{/switch}}
  {{/each}}
{{/ifEmpty}}
}