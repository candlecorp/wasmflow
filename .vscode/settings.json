{
  "git.ignoreLimitWarning": true,
  "yaml.schemas": {
    "crates/wasmflow/wasmflow-manifest/json-schema/v0/manifest.json": [
      "crates/wasmflow/wasmflow-runtime/manifests/v0/**/*.yaml",
      "tests/manifests/*.yaml",
      "crates/wasmflow/wasmflow-manifest/tests/manifests/v0/*.yaml"
    ]
  },
  "rust-analyzer.cargo.features": [
    "wasmflow-component/guest",
    "wasmflow-stdlib/provider"
  ],
  "rust-analyzer.rustfmt.overrideCommand": [
    "rustup",
    "run",
    "nightly",
    "--",
    "rustfmt",
    "--edition",
    "2021",
    "--"
  ]
}
