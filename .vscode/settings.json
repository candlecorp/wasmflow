{
  "git.ignoreLimitWarning": true,
  "yaml.schemas": {
    "crates/wasmflow/wasmflow-manifest/json-schema/manifest.json": ["**/*.wafl"]
  },
  "rust-analyzer.cargo.features": [
    "wasmflow-component/guest",
    "wasmflow-stdlib/host"
  ],
  "files.associations": {
    "*.wafl": "yaml"
  },
  "rust-analyzer.rustfmt.overrideCommand": [
    "rustup",
    "run",
    "nightly",
    "--",
    "rustfmt",
    "--edition",
    "2021",
    "--"
  ]
}
