{
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#bf2d17",
    "activityBar.activeBorder": "#000000",
    "statusBarItem.hoverBackground": "#8a1e0d",
    "titleBar.activeBackground": "#bf2d17",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#60170c",
    "titleBar.inactiveForeground": "#e7e7e799",
    "sash.hoverBorder": "#fa3a1d",
    "tab.activeBorder": "#0096AD",
    "tab.activeBorderTop": "#000000"
  },
  "git.ignoreLimitWarning": true,
  "yaml.schemas": {
    "crates/wick/wick-config/json-schema/manifest.json": [
      "crates/**/manifests/**/*.yaml",
      "crates/bins/wick/tests/**/*.yaml",
      "*.wick",
      "component.yaml",
      "app.yaml"
    ]
  },
  "rust-analyzer.cargo.features": [
    "wick-rpc/client",
    "wick-packet/datetime",
    "wick-packet/invocation"
  ],
  "rust-analyzer.rustfmt.overrideCommand": [
    "rustup",
    "run",
    "nightly",
    "--",
    "rustfmt",
    "--edition",
    "2021",
    "--"
  ],
  "rust-analyzer.showUnlinkedFileNotification": false,
  "[just]": {
    "editor.renderWhitespace": "all"
  }
}
